<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="itemlist">
	<insert id="input" parameterType="launch.box.makeit.itemlist.vo.ItemListVO">
		INSERT INTO itemList
		(itemSrl,sort) VALUES
		(#{itemSrl},#{sort});
	</insert>
	<select id="pullItemSrl" parameterType="String" resultType="Integer">
		SELECT itemSrl FROM itemList WHERE sort=#{sort};
	</select>
	<select id="pullItemList" parameterType="String" resultType="Integer">
		SELECT * FROM itemList WHERE sort=#{sort};
	</select>
</mapper>